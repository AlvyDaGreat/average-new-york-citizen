var gravity = 0


func _process(delta: float) -> void:
	position.y += (gravity + 1) / 2* delta
	gravity -= delta
	scale = Vector3(gravity/2 + 1,gravity/2 + 1,1)
	modulate.a = gravity + 1
	if scale.x <= 0:
		queue_free()
